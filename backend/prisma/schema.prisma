generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model user {
  id       String @id @default(uuid())
  login    String @unique
  password String
  cpf      String
  endereco String
  idade    String
  rg       String
  email    String
  name     String
}

model hospital {
  id         String @id @default(uuid())
  nome       String
  endereco   String
  cnpj       String @unique
  telefone   String
  email      String
  senha      String
  idHospital String
  login      String @unique
}
